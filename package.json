{
  "name": "find-it",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run copy:assets && next build && nest build --path ./tsconfig.server.json",
    "build:next": "npm run copy:assets && next build",
    "build:nest": "npm run copy:assets && nest build --path ./tsconfig.server.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run copy:assets && npm run build && node ./dist/server/main.js",
    "start:dev": "npm run copy:assets && nest start --path ./tsconfig.server.json --watch",
    "start:debug": "npm run copy:assets && nest start --path ./tsconfig.server.json --debug --watch",
    "start:prod": "npm run copy:assets && npm run build && node ./dist/server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy:assets": "rm -rf ./.next ./dist && mkdir ./dist && cp -r ./public/ ./dist/public/"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "express-session": "^1.17.3",
    "nest-next": "^10.1.0",
    "next": "^13.4.2",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.2",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-yandex": "^0.0.5",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sass": "^1.69.4",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@next/eslint-plugin-next": "^13.5.4",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-apple": "^2.0.2",
    "@types/passport-github": "^1.1.11",
    "@types/passport-google-oauth": "^1.0.44",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
